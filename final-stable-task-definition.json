[
  {
    "name": "snspmt-app",
    "image": "123456789012.dkr.ecr.ap-northeast-2.amazonaws.com/snspmt:latest",
    "portMappings": [
      {
        "containerPort": 8000,
        "protocol": "tcp"
      }
    ],
    "environment": [
      {
        "name": "DATABASE_URL",
        "value": "postgresql://postgres:Snspmt2024!@snspmt-cluste.cluster-cvmiee0q0zhs.ap-northeast-2.rds.amazonaws.com:5432/snspmt"
      },
      {
        "name": "PYTHONIOENCODING",
        "value": "utf-8"
      },
      {
        "name": "LANG",
        "value": "en_US.UTF-8"
      },
      {
        "name": "LC_ALL",
        "value": "en_US.UTF-8"
      },
      {
        "name": "ALLOWED_ORIGINS",
        "value": "https://sociality.co.kr,http://localhost:3000"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/snspmt-task",
        "awslogs-region": "ap-northeast-2",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]


