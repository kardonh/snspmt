{
  "serviceName": "snspmt-service-final",
  "cluster": "snspmt-cluster",
  "scalingPolicies": [
    {
      "name": "cpu-scaling-up",
      "metricType": "CPUUtilization",
      "targetValue": 70,
      "scaleOutCooldown": 300,
      "scaleInCooldown": 300,
      "minCapacity": 1,
      "maxCapacity": 10
    },
    {
      "name": "memory-scaling-up", 
      "metricType": "MemoryUtilization",
      "targetValue": 80,
      "scaleOutCooldown": 300,
      "scaleInCooldown": 300,
      "minCapacity": 1,
      "maxCapacity": 10
    }
  ],
  "targetTrackingScalingPolicies": [
    {
      "targetValue": 70.0,
      "scaleOutCooldown": 300,
      "scaleInCooldown": 300,
      "disableScaleIn": false
    }
  ]
}
